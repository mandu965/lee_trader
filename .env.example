# Copy this file to .env and fill in your real secrets before running docker compose
# IMPORTANT: Do NOT commit your real .env to version control

# KIS virtual trading base URL (VTS). For real trading, change to production URL as needed.
KIS_BASE_URL=https://openapivts.koreainvestment.com:29443

# Korea Investment & Securities API credentials (KEEP SECRET)
KIS_APP_KEY=YOUR_APP_KEY
KIS_APP_SECRET=YOUR_APP_SECRET
KIS_APP_ID=YOUR_ACCOUNT_ID
KIS_APP_PASSWORD=YOUR_ACCOUNT_PASSWORD

# Node runtime environment
NODE_ENV=development

# Optional: default symbols universe (comma-separated)
SYMBOLS=005930,000660,035420,051910,207940,068270,035720,005380,066570,005490
